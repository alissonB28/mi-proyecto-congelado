text = "🧠 ¿Qué significa esto en mecánica cuántica?"

[node name="Label3" type="Label" parent="HBoxContainer/ScrollContainer/body/pages/rendija_t/body/Panel/fase_3"]
layout_mode = 0
offset_left = -2.0
